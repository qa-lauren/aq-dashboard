$('#jenkins-refresh-<%= @build.id %>').click(function () {
   $('#build-card-<%= @build.id %>').replaceWith('<%= j render partial: "build_card", locals: {build: @build} %>');
   return false;
});

setTimeout(function () {
  $('#jenkins-refresh-<%= @build.id %>').unbind('click');
}, 30000);


$.notify({
  message: "<%= flash[:info] %>"
},{
  type: "info"
});
